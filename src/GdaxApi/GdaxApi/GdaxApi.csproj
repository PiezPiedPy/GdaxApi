<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/rotiv123/GdaxApi</RepositoryUrl>
    <PackageProjectUrl>https://github.com/rotiv123/GdaxApi</PackageProjectUrl>
    <PackageTags>gdax coinbase api</PackageTags>
    <PackageVersion>0.0.0</PackageVersion>
    <AssemblyVersion>0.0.0.0</AssemblyVersion>
    <FileVersion>0.0.0.0</FileVersion>
    <Version>0.0.0</Version>
    <Description>A .NET client for the GDAX REST API https://api.gdax.com.</Description>
    <Authors>Rotiv</Authors>
    <PackageLicenseUrl>https://github.com/rotiv123/GdaxApi/blob/master/LICENSE</PackageLicenseUrl>
    <PackageReleaseNotes />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>
